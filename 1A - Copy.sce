clc; clear;
disp("Quadratic equation. roots of equation 5x^2 -2x -8 = 0");
a = 5; b = -2; c = 8;d = b^2 - 4 * a * c
X1 = (-d + sqrt(d))/(2*a)
X2 = (-d - sqrt(d))/(2*a)
if d>0 then
    disp("roots are imaginary",[X1,X2])
else 
    disp("roots are real and unequal",[X1, X2])
end

