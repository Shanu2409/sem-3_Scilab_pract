//7(a) 
clc;clear;
disp("Euler Method")
deff ('z=f(x,y)','z=x-y^2')
x0=0; xn=1; y0=1; h=0.1;
n=(xn-x0)/h
for i=1:n
    y1=y0+h*f(x0,y0);x0=x0+h;
    disp([x0,y1])
    y0=y1
end

